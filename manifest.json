{
  "manifest_version": 2,
"name": "Moodle Login Plugin",
"description": "Click the login to Moodle button automatically",
"version": "1.0",
"content_scripts":[
    {
      "matches":["https://able.moodle.maynoothuniversity.ie/login/index.php"],
	  "css": ["styles.css"],
      "js": ["content.js"]
    }
  ],
"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
"background": {
      "scripts": ["background.js"],
      "persistent": false
    },
"permissions": [
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/"
  ]
}